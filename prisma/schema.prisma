generator client {
  provider = "prisma-client-js"
  binaryTargets = ["native", "rhel-openssl-3.0.x"]
}

datasource db {
  provider = "postgresql"
}

model Contact {
  id        String   @id @default(cuid())
  name      String
  email     String
  lookingFor String
  createdAt DateTime @default(now())
}